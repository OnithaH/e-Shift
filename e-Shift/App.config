<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>

	<connectionStrings>
		<add name="eShiftDB"
             connectionString="Data Source=localhost;Initial Catalog=eShiftDB;Integrated Security=True;TrustServerCertificate=True"
             providerName="Microsoft.Data.SqlClient" />
	</connectionStrings>

	<appSettings>
		<add key="ApplicationName" value="e-Shift Management System" />
		<add key="Version" value="1.0.0" />
	</appSettings>
</configuration>